{{author-detail author=model}}
 {{!--#each model as |author|}}
 
 {{author-detail name=author.name id=author.id title=author.title dateOfBirth=author.dateOfBirth dateOfDeath=author.dateOfDeath}} 
 {{/each--}} 
     {{#each model as |author|}}
  
    <h2>{{author.name}}</h2>
    
    <p>
        <strong>Id Autor:</strong> {{author.id}}</p>
    <p>
        <strong>Fecha de Nacimiento:</strong> {{author.dateOfBirth}}</p>
    <p>
        <strong>Fecha de Muerte:</strong> {{author.dateOfDeath}}</p>
    <p>
        <strong>Libros:</strong>
        <ul>
            {{#each author.books as |books|}}
            <li>{{books.title}}</li>
            {{/each}}
        </ul>
        <h3>La cantidad total de libros que ha escrito es :
            <strong>{{author.books.length}}</strong>
        </h3>
    </p>
    {{/each}}
{{outlet}}

